#Group By
query TI
SELECT g, try_sum(x) AS sum_x
FROM VALUES
  ('bad', CAST(9223372036854775807 AS BIGINT)),
  ('bad', 1),
  ('ok', 10),
  ('ok', NULL),
  ('ok', 5) AS tab(g, x)
GROUP BY g
ORDER BY g;
----
bad NULL
ok 15
