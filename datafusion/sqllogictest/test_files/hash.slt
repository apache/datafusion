# Test xxhash32 with string input
query T
SELECT xxhash32('foo') AS hash_value;
----
e20f0dd9

# Test xxhash32 with array input
query T
SELECT xxhash32(column1) AS xxhash32_result FROM ( SELECT UNNEST(ARRAY[1, 2, 3, 4, 5]) AS column1 ) AS subquery;
----
b6ecc8b2
d43589af
b6855437
01543429
b30d56b4

# Test xxhash32 with Utf8View array input
query T
WITH input_data AS (SELECT arrow_cast(column1, 'Utf8View') as utf8view_value FROM (VALUES ('foobar1'),('foobar2'),('foobar3')) AS t(column1)) SELECT xxhash32(utf8view_value) as hash_value FROM input_data;
----
2b0d1874
9925f907
df748f36

query T
SELECT xxhash32(NULL) AS hash_value;
