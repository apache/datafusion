statement ok
CREATE TABLE unnest_table
AS VALUES
    ([[1,2,3]], [struct([1,2,3],[4,5,6])], [13, 14], struct(1,2)),
    -- null array to verify the `preserve_nulls` option
    (null, null, [17, 18], null)
;

query II
select unnest(unnest(column1)) + unnest(unnest(column1)), unnest(unnest(column2)['c0']) + unnest(unnest(column2)['c1']) from unnest_table;
----
2 [1, 2, 3]
4 [1, 2, 3]
6 [1, 2, 3]


