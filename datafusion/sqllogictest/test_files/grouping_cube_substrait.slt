statement ok
CREATE TABLE test(c1 VARCHAR, c2 VARCHAR) AS VALUES
  ('a', 'A'),
  ('b', 'B');

# grouping_with_cube
query TTIIII
select
  c1,
  c2,
  grouping(c1) as g0,
  grouping(c2) as g1,
  grouping(c1, c2) as g2,
  grouping(c2, c1) as g3
from
  test
group by
  cube(c1, c2)
order by
  c1, c2, g0, g1, g2, g3;
----
a A 0 0 0 0
a NULL 0 1 1 2
b B 0 0 0 0
b NULL 0 1 1 2
NULL A 1 0 2 1
NULL B 1 0 2 1
NULL NULL 1 1 3 3
