## Scalar Function Tests
#############

statement ok
CREATE TABLE unsigned_integers(
  a INT,
  b INT,
  c INT,
  d INT,
  e INT,
  f INT
) as VALUES
  (1, 100, 567, 1024, 4, 10),
  (2, 1000, 123, 256, 5, 11),
  (3, 10000, 978, 2048, 6, 12),
  (4, NULL, NULL, 512, NULL, NULL)
;

statement ok
CREATE TABLE signed_integers(
  a INT,
  b INT,
  c INT,
  d INT,
  e INT,
  f INT
) as VALUES
  (-1, 100, -567, 1024, -4, 10),
  (2, -1000, 123, -256, 5, -11),
  (-3, 10000, -978, 2048, -6, 12),
  (4, NULL, NULL, -512, NULL, NULL)
;

statement ok
CREATE TABLE small_floats(
  a FLOAT,
  b FLOAT,
  c FLOAT,
  d FLOAT,
  e FLOAT,
  f FLOAT
) as VALUES
  (0.2, -0.1, 1.0, -0.9, 0.1, 0.5),
  (0.5, -0.2, 0.0, 0.9, -0.2, 0.6),
  (-0.7, 0.1, -1.0, 0.9, 0.3, -0.7),
  (-1.0, NULL, NULL, -0.9, NULL, NULL)
;

