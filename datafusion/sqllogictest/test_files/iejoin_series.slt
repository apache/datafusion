CREATE TABLE lhs (
    id INT,
    begin INT,
    end INT
);

statement ok
INSERT INTO lhs (id, begin, end)
SELECT
    i AS id,
    i AS begin,
    i + 1 AS end
FROM unnest(generate_series(1, 1000001)) tbl(i);

statement ok
CREATE TABLE rhs (
    id INT,
    begin INT,
    end INT
);

statement ok
INSERT INTO rhs (id, begin, end)
SELECT
    i - 1000000 AS id,
    i AS begin,
    i + 1 AS end
FROM unnest(generate_series(1000001, 2000001)) tbl(i);

